{
    "name": "vscode-ruby-syntax",
    "displayName": "vscode_ruby_syntax",
    "description": "Extends Ruby Language Grammar",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.44.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "ruby",
            "aliases": ["ruby", "ruby"],
            "extensions": [".rb"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "ruby",
            "scopeName": "source.ruby",
            "path": "./syntaxes/ruby.tmLanguage.json"
        }]
    }
}